
<table border="2">
    <tr>
    
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Address</th>
        <th>City</th>
        <th>Gender</th>
        <th>Info</th>
        
    </tr>
    <% for (let row of output) {%>
        <tr>
            <td><%- row._id %></td>
            <td><%- row.name %></td>
            <td><%- row.email %></td>
            <td><%- row.mobile %></td>
            <td><%- row.address %></td>
            <td><%- row.city %></td>
            <td><%- row.gender %></td>
            <td><%- row.info %></td>
         </tr>
        <% } %>
</table>